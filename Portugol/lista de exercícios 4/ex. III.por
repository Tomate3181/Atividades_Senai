// 3- Crie um algoritmo que leia um número e alerte quando o
// mesmo estiver entre 100 e 500.


programa {
  funcao inicio() {
    
    inteiro numero

    escreva ("Digite um número inteiro: ")
    leia (numero)

    se (numero < 100 ou numero > 500) {
    }
    senao {
      escreva("O número digitado está entre 100 e 500!")
    }
  }
}
